{
  "name": "masters-leaderboard",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "postinstall": "lerna bootstrap",
    "test": "lerna run test",
    "test:ci": "lerna run test:ci --since refs/remotes/origin/main",
    "check-types": "lerna run check-types",
    "check-types:ci": "lerna run check-types --since refs/remotes/origin/main",
    "start": "npm run start -w packages/web",
    "start:mobile": "npm run start -w packages/mobile",
    "start:standalone": "npm run start:standalone -w packages/web",
    "server": "npm run server -w packages/web",
    "build:prod": "npm run build:prod -w packages/web",
    "build:server": "npm run build:server -w packages/web",
    "build:ui": "npm run build:ui -w packages/web",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint packages --ext .ts,.tsx",
    "lint:prettier": "prettier --check \"packages/**/*.{js,jsx,ts,tsx}\"",
    "test:coverage": "npm run test:coverage -w packages/web",
    "render:startup": "npm run render:startup -w packages/web",
    "bootstrap:ci": "lerna bootstrap --since refs/remotes/origin/main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianpburns/masters-leaderboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brianpburns/masters-leaderboard/issues"
  },
  "homepage": "https://github.com/brianpburns/masters-leaderboard#readme",
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest": "^29.2.1",
    "lerna": "^6.6.2",
    "prettier": "^3.2.5",
    "typescript": "^5.1.3"
  }
}
